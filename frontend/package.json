{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out",
    "postbuild": "next-sitemap",
    "setup:vercel": "./scripts/setup-vercel.sh",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:all": "jest --testPathPattern=__tests__",
    "build:web": "next build",
    "build:capacitor": "npm run build:web && LANG=en_US.UTF-8 npx cap sync",
    "build:capacitor:windows": "npm run build && npx cap sync android",
    "build:capacitor:ios": "npm run build:capacitor && npx cap open ios",
    "cap:sync": "LANG=en_US.UTF-8 npx cap sync",
    "cap:sync:windows": "npx cap sync",
    "cap:sync:android": "npx cap sync android",
    "cap:sync:ios": "LANG=en_US.UTF-8 npx cap sync ios",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios"
  },
  "dependencies": {
    "@capacitor-firebase/messaging": "^7.3.1",
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/clipboard": "^7.0.2",
    "@capacitor/core": "^7.4.3",
    "@capacitor/device": "^7.0.2",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ebarooni/capacitor-calendar": "^7.2.0",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.7.8",
    "capacitor-plugin-safe-area": "^4.0.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "firebase": "^11.10.0",
    "framer-motion": "^11.18.2",
    "keen-slider": "^6.8.6",
    "next": "15.5.9",
    "next-auth": "^4.24.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.66.1",
    "react-redux": "^9.2.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwind-scrollbar-hide": "^1.1.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lucide-react": "^0.294.0",
    "msw": "^2.11.1",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "description": "This is a [Next.js](https://nextjs.org) project bootstrapped with [`create-next-app`](https://nextjs.org/docs/app/api-reference/cli/create-next-app).",
